    <validationRules>
        <fullName>Can_not_Void_a_Paid_Invoice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Status__c), ISPICKVAL(Status__c, &apos;Voided&apos;), ISPICKVAL(PRIORVALUE(Status__c), &apos;Paid&apos;))</errorConditionFormula>
        <errorMessage>Can not Void a Paid Invoice</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_not_change_from_Voided_Invoice</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Status__c), ISPICKVAL(PRIORVALUE(Status__c), &apos;Voided&apos;))</errorConditionFormula>
        <errorMessage>Can not change the Status of a Voided Invoice</errorMessage>
    </validationRules>
