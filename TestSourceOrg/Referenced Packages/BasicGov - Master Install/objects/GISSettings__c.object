<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configuration for GIS interface</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AllowCallouts__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Callouts are permitted to configured GIS REST services</description>
        <externalId>false</externalId>
        <inlineHelpText>If true, callouts are permitted to call external GIS REST services (ensure they hosted as valid Remote Sites)</inlineHelpText>
        <label>Allow Callouts</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GeoCodeService__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>URL for the GeoCode REST Service (include any default parameters)
eg. http://sampleserver1.arcgisonline.com/ArcGIS/rest/services/Locators/ESRI_Geocode_USA/GeocodeServer/findAddressCandidates?Address=</inlineHelpText>
        <label>DefaultGeoCodeService</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ParcelQueryFields__c</fullName>
        <defaultValue>&quot;PARCELID&quot;</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Parcel field containing the parcel number eg. Name
or PARCELID
or Name,PARCELID</inlineHelpText>
        <label>ParcelQueryFields</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ParcelQueryService__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>The Map Server Parcel Layer Query REST Service (incl. default params) Layer must contain Parcel Number
eg. http://host/ArcGIS/rest/services/Louisville/LOJIC_LandRecords_Louisville/MapServer/0/query?returnGeometry=true&amp;geometryType=esriGeometryPoint&amp;text=</inlineHelpText>
        <label>ParcelQueryService</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>GIS Settings</label>
    <visibility>Public</visibility>
</CustomObject>
