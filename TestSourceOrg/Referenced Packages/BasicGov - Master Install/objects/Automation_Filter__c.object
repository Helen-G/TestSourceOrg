    <validationRules>
        <fullName>Value_Check_Syntax</fullName>
        <active>true</active>
        <description>Check syntax</description>
        <errorConditionFormula>OR(
   CONTAINS(Value__c, &quot;&lt;content&gt;&quot;) &amp;&amp; !REGEX(Value__c, &quot;[a-zA-Z\\s\\d]*(\\&lt;content\\&gt;\\w+(\\.\\w+){1,2}\\&lt;/content\\&gt;)?[a-zA-Z\\s\\d]*&quot;),
   CONTAINS(Value__c, &quot;\\{\\!&quot;) &amp;&amp; !REGEX(Value__c, &quot;[a-zA-Z\\s\\d]*(\\{\\!\\$?\\w+(\\.\\w+){1,2}\\})?[a-zA-Z\\s\\d]*&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Value__c</errorDisplayField>
        <errorMessage>Invalid Filter Value syntax</errorMessage>
    </validationRules>
