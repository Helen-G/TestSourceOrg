        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IMAGE( 
IF(AND(ISNULL(End_Date__c), Due_Date__c &lt; TODAY()), &quot;/img/entitlement/casemilestone_isviolated.png&quot;, 
   IF(AND(ISNULL(End_Date__c), Due_Date__c &lt; Last_Task_Due_Date__c), &quot;https://letterbatch.mybasicgov.com/img/redwarning.gif&quot;,
    IF (End_Date__c &lt;= Due_Date__c, &quot;/img/entitlement/casemilestone_iscompleted.gif&quot;, 
      IF (End_Date__c &gt; Due_Date__c, &quot;/img/icon/warning16.png&quot;, 
   &quot;/s.gif&quot;
   )
  )
 )
),
&quot;&quot;
)</formula>
