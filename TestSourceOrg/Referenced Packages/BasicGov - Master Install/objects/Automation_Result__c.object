    <validationRules>
        <fullName>Value_Check_Syntax</fullName>
        <active>true</active>
        <description>Check syntax</description>
        <errorConditionFormula>OR( 
 ISBLANK(Value__c), 
 CONTAINS(Value__c, &quot;&lt;content&quot;) 
     &amp;&amp; !REGEX(Value__c, &apos;[a-zA-Z\\s\\d]*(&lt;content(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*&gt;\\w+(\\.\\w+){1,2}&lt;/content&gt;)?[a-zA-Z\\s\\d]*&apos;) 
     &amp;&amp; !REGEX(Value__c, &apos;[a-zA-Z\\s\\d]*(&lt;content-diff(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*&gt;\\w+(\\.\\w+){1,2}&lt;/content-diff&gt;)?[a-zA-Z\\s\\d]*&apos;), 
/*
 CONTAINS(MUSW__Value__c, &quot;{!&quot;) 
    &amp;&amp; !REGEX(MUSW__Value__c,&apos;[a-zA-Z\\s\\d]*(\\{\\!\\$?\\w+(\\.\\w+){1,2\\)?[a-zA-Z\\s\\d]*&apos;), 
*/
 CONTAINS(Value__c, &quot;&lt;today&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;today(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*/&gt;&apos;), 
 CONTAINS(Value__c, &quot;&lt;now&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;now/&gt;&apos;), 
 CONTAINS(Value__c, &quot;&lt;user&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;user/&gt;&apos;), 
 CONTAINS(Value__c, &quot;&lt;formula&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;formula&gt;.*&lt;/formula&gt;&apos;), 
 ISPICKVAL(Action_Type__c,&quot;Add Letter&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;letter(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*/&gt;&apos;), 
 ISPICKVAL(Action_Type__c,&quot;Send Email&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;email(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*/&gt;&apos;), 
 ISPICKVAL(Action_Type__c,&quot;Email Report&quot;) &amp;&amp; !REGEX(Value__c, &apos;&lt;report(\\s+\\w+=&quot;?[\\w\\s]+&quot;?)*/&gt;&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Value__c</errorDisplayField>
        <errorMessage>Invalid Result Value syntax for selected Action Type</errorMessage>
    </validationRules>
